<h1 class="mt-4">Dashboard</h1>
<p class="lead mb-3">Welcome <%= user.name %></p>
<div class="text-danger"><%= errInfo %></div>
<div>Current Active Sessions:</div>
<div>SessionID: <%= user.sessionId %></div>
<div>UserID: <%= user.id %></div>
<div><a href="/admin/revoke/<%= user.sessionId %>">Revoke Session</a></div>
<p class="mt-3">Other Active Sessions:</p>
<% sessions.forEach(function(session) { %>
  <div>SessionID: <%= session.sessionId %></div>
  <div>UserID: <%= session.user %></div>
  <div><a href="/admin/revoke/<%= session.sessionId %>">Revoke Session</a></div>
<% }); %>
